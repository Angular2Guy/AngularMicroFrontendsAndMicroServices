{
  "name": "flightselection",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build-stats": "ng build --configuration production --stats-json",
    "watch": "ng build --watch --configuration development",
    "analyse": "webpack-bundle-analyzer dist/manager/de/stats.json",
    "test": "ng test --browsers ChromeHeadless --watch=false --code-coverage=true",
    "test-chromium": "ng test --browsers ChromiumHeadless --watch=false --code-coverage=true",
    "test-local": "ng test --browsers Chromium --watch=true",
    "prebuild": "rimraf ./dist && mkdirp ./dist",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../../../flightSelection-backend/src/nestjs/static/*",
    "deploy": "cpy 'dist/flightselection/browser/**' ../../../flightSelection-backend/src/nestjs/static/",
    "prettier": "npx prettier --write 'src/**/*.{ts,html,scss,json}'",
    "extract-i18n": "ng extract-i18n --format=json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/material-luxon-adapter": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@jsverse/transloco": "^7.5.0",
    "@jsverse/transloco-locale": "^7.0.1",
    "es-module-shims": "^1.5.12",
    "luxon": "^3.4.0",
    "material-icons": "^1.13.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular/cli": "^19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/luxon": "^3.4.0",
    "@types/node": "^18.19.0",
    "cpy-cli": "^4.2.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mkdirp": "^1.0.4",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.2",
    "typescript": "~5.6.3",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
